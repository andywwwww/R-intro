(mat <- matrix(1:12, ncol = 4,  nrow = 3))
matrix(1:12, ncol = 4,  nrow = 3, byrow = T)
players <- c("Andy Murray", "Rafael Nadal", "Stan Wawrinka", 
             "Novak Djokovic", "Roger Federer")
grand_slam_win <- c(9, 15, 5, 12, 18)
